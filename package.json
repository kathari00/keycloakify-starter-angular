{
  "name": "keycloakify-starter-angular",
  "version": "0.0.1",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "ng": "ng",
    "build": "ng build -c production",
    "postbuild": "rimraf build/keycloak-resources",
    "test": "ng test",
    "serve": "ng serve",
    "build-keycloak-theme": "ng build -c production && keycloakify build",
    "_format": "prettier '**/*.{ts,json,html}'",
    "format": "npm run _format --write",
    "lint": "ng lint --fix"
  },
  "keycloakify": {
    "projectBuildDirPath": "build",
    "staticDirPathInProjectBuildDirPath": "static",
    "publicDirPath": "src/keycloak-theme/assets",
    "accountThemeImplementation": "none"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.0",
    "@angular/common": "^18.2.0",
    "@angular/compiler": "^18.2.0",
    "@angular/core": "^18.2.0",
    "@angular/forms": "^18.2.0",
    "@angular/platform-browser": "^18.2.0",
    "@angular/router": "^18.2.0",
    "keycloakify": "^10.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular-builders/custom-esbuild": "^18.0.0",
    "@angular-devkit/build-angular": "^18.2.0",
    "@angular/cli": "^18.2.0",
    "@angular/compiler-cli": "^18.2.0",
    "@types/node": "^20.0.0",
    "angular-eslint": "^18.3.1",
    "eslint": "^9.9.1",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "^3.3.3",
    "rimraf": "^5.0.7",
    "typescript": "~5.4.2",
    "typescript-eslint": "^8.2.0"
  }
}
